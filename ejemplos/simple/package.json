{
  "name": "plantilla-web",
  "version": "0.0.1",
  "description": "Plantilla para iniciar proyectos web rápidamente.",
  "browser": "dist/index.js",
  "author": "Juan Camilo González Jiménez <info@juancgonzalez.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
